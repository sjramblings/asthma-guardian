# AWS Configuration
AWS_REGION=ap-southeast-2
AWS_ACCOUNT_ID=123456789012

# Application Configuration
APP_NAME=asthma-guardian-v3
ENVIRONMENT=dev

# Database Configuration
DYNAMODB_TABLE_PREFIX=asthma-guardian

# API Configuration
API_DOMAIN=api-dev.asthmaguardian.nsw.gov.au
FRONTEND_DOMAIN=dev.asthmaguardian.nsw.gov.au

# Monitoring
LOG_LEVEL=INFO
ENABLE_XRAY=true

# Security
JWT_SECRET_KEY=your-jwt-secret-key-here
ENCRYPTION_KEY_ID=your-kms-key-id-here

# External APIs
NSW_AIR_QUALITY_API_URL=https://api.airquality.nsw.gov.au
BOM_API_URL=https://api.bom.gov.au

# Notification Services
SES_FROM_EMAIL=noreply@asthmaguardian.nsw.gov.au
SNS_TOPIC_ARN=arn:aws:sns:ap-southeast-2:123456789012:asthma-guardian-notifications

# Development
DEBUG=true
RELOAD=true
